<?xml version="1.0" encoding="utf-8"?>
<resources>


    <attr name="color_no_finger_inner_circle" format="color" />
    <attr name="color_no_finger_outer_circle" format="color" />
    <attr name="color_finger_on" format="color" />
    <attr name="color_finger_up" format="color" />
    <attr name="count" format="integer" />
    <attr name="tryTimes" format="integer" />

    <declare-styleable name="GestureLockViewGroup">
    <attr name="color_no_finger_inner_circle" />
    <attr name="color_no_finger_outer_circle" />
    <attr name="color_finger_on" />
    <attr name="color_finger_up" />
    <attr name="count" />
    <attr name="tryTimes" />
    </declare-styleable>

    <declare-styleable name="AppRadioButton">

        <attr name="unReadNumber" format="integer|reference"></attr>

        <attr name="unReadBackColor" format="color|reference" />

        <attr name="unReadTextColor" format="color|reference" />

        <attr name="unReadTextSize" format="dimension|reference" />

        <attr name="unReadCircleRadius" format="dimension|reference" />

        <attr name="unReadCircleRightOffset" format="dimension|reference" />
        <attr name="unReadCircleTopOffset" format="dimension|reference" />

    </declare-styleable>

    <declare-styleable name="FlowBaseLayout">

        <attr name="maxLines" format="integer">
            <enum name="nolimit" value="-1" />
        </attr>
        <attr name="maxColumns" format="integer">
            <enum name="nolimit" value="-1" />
        </attr>
        <attr name="columnmargin" format="dimension"></attr>
        <attr name="linemargin" format="dimension"></attr>
        <attr name="tagHeight" format="dimension|reference"></attr>

    </declare-styleable>

    <declare-styleable name="MultiViewLayout">
        <attr name="columnDividerColor" format="color" />
        <attr name="rowDividerColor" format="color" />
        <attr name="columnDividerHeight" format="dimension" />
        <attr name="rowDividerHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleButtonView">
        <attr name="minTime" format="integer"></attr>
        <attr name="maxTime" format="integer"></attr>
        <attr name="progressColor" format="color"></attr>
        <attr name="progressWidth" format="dimension"></attr>
    </declare-styleable>

</resources>